@model IEnumerable<CoreMultiJoinsProject.Models.States>
@{
    ViewData["Title"] = "FilterCountry";
}

<h1>FilterState</h1>

<ul>
    @foreach (var gc in Model)
    {
        <li>
            <a asp-controller="Add" asp-action="FilterCities" asp-route-id="@gc.Id">@gc.StateName</a>
        </li>

    }
</ul>