
@model IEnumerable<CoreMultiJoinsProject.Models.Cities>
@{
    ViewData["Title"] = "FilterCities";
}

<h1>FilterCities</h1>

<ul>
    @foreach (var gc in Model)
    {
        <li>
            <a asp-controller="Add" asp-action="EmployeeDetails" asp-route-id="@gc.Id">@gc.CityName</a>
        </li>

    }
</ul>